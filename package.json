{
  "name": "codevandafull",
  "version": "1.0.0",
  "description": "Container for both front-end and server",
  "main": "server.js",
  "dependencies": {
	"node": "^10.16.3",
	"npm": "^6.11.3",
    "concurrently": "^6.0.0",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "react-iframe": "^1.8.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start-js": "cd Client && npm start",
	"build-js": "cd Client && npm run-script build",
	"production-js": "cd Client && serve -s build",
    "backend-start": "node server.js",
    "start": "concurrently \"npm-run-all -p backend-start start-js\"",
    "production": "concurrently \"npm-run-all -p backend-start build-js\"",
	"build": "concurrently \"npm-run-all -p backend-start production-js\"",
    "test": "cd Client && npm test",
    "eject": "cd Client && npm eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k3nn3thh3ng/CodeVanda.git"
  },
  "author": "kenneth",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/k3nn3thh3ng/CodeVanda/issues"
  },
  "homepage": "https://github.com/k3nn3thh3ng/CodeVanda#readme"
}
